{
    "name": "advalis/laravel-playwright",
    "description": "Laravel and Playwright E2E Testing",
    "keywords": ["laravel", "playwright", "testing", "e2e"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Advalis\\LaravelPlaywright\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Advalis\\LaravelPlaywright\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Advalis",
            "email": "dev@advalis.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^2.0",
        "larastan/larastan": "^3.0",
        "phpunit/phpunit": "^10.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Advalis\\LaravelPlaywright\\LaravelPlaywrightServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
